{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Project",
  "type": "object",
  "required": [
    "description",
    "funding_raised",
    "funding_requested",
    "id",
    "title"
  ],
  "properties": {
    "description": {
      "type": "string"
    },
    "funding_raised": {
      "type": "integer",
      "format": "uint128",
      "minimum": 0.0
    },
    "funding_requested": {
      "type": "integer",
      "format": "uint128",
      "minimum": 0.0
    },
    "id": {
      "type": "integer",
      "format": "uint128",
      "minimum": 0.0
    },
    "title": {
      "type": "string"
    }
  }
}
